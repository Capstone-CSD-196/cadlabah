{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
      "linebreak-style": "off",
      "no-console": "off",
      "no-underscore-dangle": "off",
      "quote-props": "off",
      "no-plusplus": "off",
      "no-undef": "off",
      "func-names": "off"
    }
}
